{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info",
    "request": {
      "payload": {
        "limit": "10mb"
      }
    }
  },
  "server": {
    "port": "8080"
  },
  "typeOrm": {
    "type": "postgres",
    "host": "",
    "port": 5432,
    "username": "",
    "password": "",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "discrete-ingestion",
    "synchronize": false,
    "logging": false,
    "entities": ["**/DAL/entity/**/*.js"],
    "migrations": [],
    "subscribers": ["**/DAL/subscriber/**/*.js"],
    "cli": {
      "entitiesDir": "src/DAL/entity",
      "migrationsDir": "src/DAL/migration",
      "subscribersDir": "src/DAL/subscriber"
    }
  }
}
